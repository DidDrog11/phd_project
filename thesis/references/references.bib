	pages = {2542--2551},
	file = {Diarra et al. - 2020 - Molecular Detection of Microorganisms Associated w.pdf:C\:\\Users\\ucbtds4\\Zotero\\storage\\3CHAEH6V\\Diarra et al. - 2020 - Molecular Detection of Microorganisms Associated w.pdf:application/pdf},
