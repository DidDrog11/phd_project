	keywords = {Animals, Mice, epidemiology, Polymerase Chain Reaction, Senegal, *Rodentia/vi [Virology], *Phylogeny, RNA, Viral/ip [Isolation \& Purification], human diseases, strains, disease vectors, vectors, *Flavivirus Infections/ve [Veterinary], *Flavivirus/ge [Genetics], *Flavivirus/ip [Isolation \& Purification]},
	pages = {181},
