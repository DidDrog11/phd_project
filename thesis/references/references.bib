	keywords = {Agriculture, Rodents, public health, Metaanalysis, Crops, Pest control, Population ecology, Predation, Seedlings, meta-analysis, methodology, health, efficacy, techniques, communities, control methods, adverse effects, crop losses, damage, data analysis, farming systems, feeding, feeding habits, guidelines, losses, pests, seedlings, vertebrate pests},
	pages = {e0174554},
