	journaltitle = {{PNAS} Nexus},
	shortjournal = {{PNAS} Nexus},
	author = {Safronetz, David and Rosenke, Kyle and Meade-White, Kimberley and Sloan, Angela and Maiga, Ousmane and Bane, Sidy and Martellaro, Cynthia and Scott, Dana P and Sogoba, Nafomon and Feldmann, Heinz},
	urldate = {2022-09-01},
	date = {2022-07-01},
	file = {Full Text PDF:C\:\\Users\\ucbtds4\\Zotero\\storage\\GJDDN9LH\\Safronetz et al. - 2022 - Temporal analysis of Lassa virus infection and tra.pdf:application/pdf;Snapshot:C\:\\Users\\ucbtds4\\Zotero\\storage\\QJEACZBD\\6643342.html:text/html},
