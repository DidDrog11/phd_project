	year = {2021},
	note = {\_eprint: https://besjournals.onlinelibrary.wiley.com/doi/pdf/10.1111/1365-2656.13595},
	keywords = {coinfection, conservation medicine, ecoimmunology, host-parasite interactions, One Health, syndemics},
