	year = {2015},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/ece3.1404},
	keywords = {One Health, evolution, dispersal, Disease ecology, metacommunity, phylogenetic structure, stochastic event},
	pages = {865--873},
	file = {Full Text PDF:/Users/david/Zotero/storage/M7T955UX/Suzán et al. - 2015 - Metacommunity and phylogenetic structure determine.pdf:application/pdf;Snapshot:/Users/david/Zotero/storage/YTU2KJNS/ece3.html:text/html},
}

@article{allen_global_2017,
